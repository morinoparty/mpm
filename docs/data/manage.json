{
    "commands": [
        {
            "command": "mpm init [[options]]",
            "status": "newly",
            "category": "manage",
            "section": "初期化",
            "description": "mpm.jsonを生成し、すべてのプラグインをunmanagedとして追加します",
            "arguments": [
                {
                    "name": "--overwrite",
                    "required": false
                }
            ]
        },
        {
            "command": "mpm add <pluginName> [[options]]",
            "status": "stable",
            "category": "manage",
            "section": "インストール・追加",
            "description": "リポジトリファイルからプラグインを管理対象に追加します（依存関係も含む）",
            "arguments": [
                {
                    "name": "pluginName",
                    "required": true
                },
                {
                    "name": "--no-deps",
                    "required": false
                },
                {
                    "name": "--soft",
                    "required": false
                }
            ]
        },
        {
            "command": "mpm adopt [[options]]",
            "status": "newly",
            "category": "manage",
            "section": "インストール・追加",
            "description": "unmanagedプラグインをリポジトリから検索してmanaged状態に変更します",
            "arguments": [
                {
                    "name": "--soft",
                    "required": false
                },
                {
                    "name": "--dry-run",
                    "required": false
                }
            ]
        },
        {
            "command": "mpm install",
            "status": "stable",
            "category": "manage",
            "section": "インストール・追加",
            "description": "`mpm-config.json`に記載されたすべてのプラグインをインストールします"
        },
        {
            "command": "mpm uninstall <pluginName>",
            "status": "stable",
            "category": "manage",
            "section": "アンインストール・削除",
            "description": "プラグインをアンインストールします",
            "arguments": [
                {
                    "name": "pluginName",
                    "required": true
                }
            ]
        },
        {
            "command": "mpm remove <pluginName>",
            "status": "newly",
            "category": "manage",
            "section": "アンインストール・削除",
            "description": "プラグインを管理対象から除外します（ファイルは削除されません）",
            "arguments": [
                {
                    "name": "pluginName",
                    "required": true
                }
            ]
        },
        {
            "command": "mpm removeUnmanaged",
            "status": "newly",
            "category": "manage",
            "section": "アンインストール・削除",
            "description": "mpm管理下にないプラグインを削除します"
        },
        {
            "command": "mpm list [[options]]",
            "status": "newly",
            "category": "manage",
            "section": "一覧表示",
            "description": "インストールされているプラグインの一覧を表示します",
            "arguments": [
                {
                    "name": "--all",
                    "required": false
                },
                {
                    "name": "--managed",
                    "required": false
                },
                {
                    "name": "--unmanaged",
                    "required": false
                },
                {
                    "name": "--locked",
                    "required": false
                }
            ]
        },
        {
            "command": "mpm outdated <plugin>",
            "status": "newly",
            "category": "manage",
            "section": "更新管理",
            "description": "プラグインの更新を確認します",
            "arguments": [
                {
                    "name": "plugin",
                    "required": true
                }
            ]
        },
        {
            "command": "mpm outdatedAll",
            "status": "newly",
            "category": "manage",
            "section": "更新管理",
            "description": "すべてのプラグインの更新を確認します"
        },
        {
            "command": "mpm versions <plugin> [[--limit]]",
            "status": "newly",
            "category": "manage",
            "section": "更新管理",
            "description": "プラグインの利用可能なバージョン一覧を表示します（デフォルト: 20件まで）",
            "arguments": [
                {
                    "name": "plugin",
                    "required": true
                },
                {
                    "name": "--limit",
                    "required": false
                }
            ]
        },
        {
            "command": "mpm update",
            "status": "newly",
            "category": "manage",
            "section": "更新管理",
            "description": "新しいバージョンがあるプラグインを更新します"
        },
        {
            "command": "mpm lock <plugin>",
            "status": "newly",
            "category": "manage",
            "section": "更新管理",
            "description": "プラグインをロックして自動更新を防ぎます",
            "arguments": [
                {
                    "name": "plugin",
                    "required": true
                }
            ]
        },
        {
            "command": "mpm unlock <plugin>",
            "status": "newly",
            "category": "manage",
            "section": "更新管理",
            "description": "プラグインのロックを解除します",
            "arguments": [
                {
                    "name": "plugin",
                    "required": true
                }
            ]
        }
    ]
}
